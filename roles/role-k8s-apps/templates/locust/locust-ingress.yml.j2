apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: locust-ingress
  namespace: {{LOCUST_NAMESPACE}}
  annotations:
    kubernetes.io/ingress.class: {{LOCUST_INGRESS_CLASS}}    
spec:
  rules:
  - host: {{LOCUST_INGRESS_HOST}}
    http:
      paths:
      - pathType: Prefix
        path: "/"
        backend:
          service: 
            name: locust
            port: 
             number: 8089